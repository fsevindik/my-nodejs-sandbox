<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Products</title>
</head>
<body>
    <div class="products">
        <% products.forEach(product => { %>
            <div>
                <!-- route  dynemic  -->
                <a href="detail/ <%= product.id %>">
                    <%= product.name %> - $<%= product.price %>
                </a><br>
            </div>
        <% }) %>
    </div>
</body>
</html>
